{
  "name": "blasteroids",
  "version": "1.0.0",
  "description": "A web-based Asteroids-style game built with TypeScript and HTML5 Canvas",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install",
    "format": "prettier --write 'src/**/*.{ts,js,json,css,md}'"
  },
  "keywords": [
    "game",
    "asteroids",
    "typescript",
    "canvas",
    "vite"
  ],
  "author": "Gabe Johnson",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
